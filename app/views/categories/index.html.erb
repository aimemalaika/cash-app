<h1> List of Categories <%=  link_to "New Enpense", new_expense_path %> </h1>
<% @categories.each do |category| %>
  <%= image_tag category.icon_url, height: "50px" %>
  <%= category.Name %>
  <%= link_to "Show", category %>
  <%= button_to "Delete", category, method: :delete, data: { confirm: "Are you sure?" } %>
<% end %>

